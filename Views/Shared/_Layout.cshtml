<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <title>@ViewData["Title"] - BuildingMaintenance</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/styles.css" />
    <link rel="stylesheet" href="~/css/dataTables.bootstrap4.min.css" />
    <link href="~/css/jquery.toast.css" rel="stylesheet" />

    <script src="~/js/jquery-3.5.1.js"></script>
    <script src="~/js/font-awesome-all.5.15.1.js"></script>

    @*<script src="js/jquery.min.js" type="text/javascript"></script>
    <script src="js/jquery.dataTables.min.js" type="text/javascript"></script>*@


</head>
<body>
    <header>
        @Html.Partial("_TopNavBar")
    </header>

    <div id="layoutSidenav">
        <div id="layoutSidenav_nav">
            @await Component.InvokeAsync("CPSideMenuNavigation")
        </div>
        <div id="layoutSidenav_content">
            <div class="container">
                <main role="main" class="pb-3">
                    @RenderBody()
                </main>
            </div>
        </div>
    </div>


    <footer class="border-top text-muted">
        <div class="container">
            &copy; 2020 - BuildingMaintenance - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/jquery.dataTables.min.js"></script>
    <script src="~/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/js/jquery.toast.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/scripts.js" asp-append-version="true"></script>
    <script src="~/js/moment.js"></script>
    <script src="~/js/daterangepicker.js"></script>

    @RenderSection("Scripts", required: false)
    <script>
        $('[type="checkbox"]').click(function () {
            $(this).val($(this).is(':checked'));
            $(this).prop('checked', $(this).is(':checked'));
        });
    </script>
</body>
</html>
